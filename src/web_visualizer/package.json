  "scripts": {
    "release_version": "echo ${npm_package_version} > ./release/web_visualizer_version"

  , "build_js":  "python3 ../jsstitcher ./src/main.js > ./release/web_visualizer.js"
  , "build_css": "powershell -Command \"Get-ChildItem -Recurse -Filter *.css ./src | Get-Content | Set-Content ./release/web_visualizer.css\""
  , "build_license": "powershell -Command \"'This file is generated by package.json' | Set-Content ./release/LICENSE; Get-ChildItem -Recurse -Filter LICENSE | Where-Object { $_.FullName -notlike '*release*' } | Get-Content | Add-Content ./release/LICENSE\""
  , "build_system": "npm run build_js && npm run build_css && npm run build_license"

    , "compile": "npm run build_system"
    , "postcompile": "npm run release_version"
